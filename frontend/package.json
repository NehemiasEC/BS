{
  "name": "frontend",
  "version": "1.0.0",
  "description": "BS frontend",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ../backend/static/*.css && rm -rf ../backend/static/*.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "prod": "npm run clean && npm run build && npm run copy-buildfiles",
    "dev": "webpack-dev-server --config webpack/webpack.dev.js --open --colors",
    "copy-buildfiles": "npm run copy-build-js && npm run copy-build-css",
    "copy-build-logo": "copyfiles -f 'build/static/media/*' '../staticfiles/img/'",
    "copy-build-js": "copyfiles -f 'dist/static/*.js' '../backend/static/'",
    "copy-build-css": "copyfiles -f 'dist/static/*.css' '../backend/static/'"
  },
  "author": "NehemiasEC",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "redux-devtools-extension": "^2.13.8",
    "renamer": "^1.1.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-bundle-tracker": "^0.4.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "d3": "^5.16.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-alert": "^7.0.1",
    "react-alert-template-basic": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "thunk": "0.0.1"
  }
}
